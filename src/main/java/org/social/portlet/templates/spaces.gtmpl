#{param name=spaces/}

<% for(space in spaces) { %>
    <span>
       <div class="well">
           <img class="avatar" src="${space.avatarUrl}"/>
             <a href="${space.url}">${space.displayName}</a>
             <div>
             Description: ${space.description}
             </div>
       </div>
    </span>
<% } %>
